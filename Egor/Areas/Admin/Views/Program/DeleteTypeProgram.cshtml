@model Egor.Models.TypeProgram
@{
	ViewData["Title"] = "Удаление типа программы";
}
<h3 class="text-danger">Вы уверены что хотите удалить тип программы вместе с его дисциплинами?</h3>
<form method="post" role="form"> 
	<input type="hidden" name="Id" value="@Model.Id"/>
	<input type="hidden" name="ProfileId" value="@Model.ProfileId"/>
	<input type="hidden" name="Name" value="@Model.Name"/>
	<button class="btn-danger btn">Удалить</button>
</form>
<a asp-action="Index" asp-route-id="@Model.ProfileId" class="btn btn-outline-secondary mt-3">Назад</a>